		<xsd:annotation>
			<xsd:documentation>Root = 999999999, Program ID = RP or RZ, Account # = 9999</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{9}[Rr][PpZz]\d{4}"/>
